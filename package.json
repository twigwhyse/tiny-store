{
  "name": "@eoyo/tiny-store",
  "version": "1.0.0",
  "description": "A tiny, type-safe React state management library with immutable operations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --config tsconfig.app.json --format cjs,esm --dts",
    "dev": "tsup src/index.ts --config tsconfig.app.json --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "examples": "vite --config vite.config.examples.ts --open",
    "examples:build": "vite build --config vite.config.examples.ts"
  },
  "keywords": [
    "react",
    "state-management",
    "typescript",
    "immutable",
    "zustand",
    "store"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "@types/react": ">=18.0.0",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/immutable": "^3.8.7",
    "@types/node": "18",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^1.0.0",
    "immer": "^10.1.1",
    "immutable": "^5.1.3",
    "jsdom": "^23.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "zustand": "^5.0.6"
  }
}
